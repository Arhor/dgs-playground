scalar Long

type Query {
    user(username: String): UserDto
    users: [UserDto]
}

type Mutation {
    createUser(username: String): UserDto
    createUserPost(userId: Long, content: String): PostDto
}

type UserDto {
    id: Long
    username: String
    posts: [PostDto]
}

type PostDto {
    id: Long
    userId: Long
    content: String
}
